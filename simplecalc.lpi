<?xml version="1.0" encoding="UTF-8"?>
<CONFIG>
  <ProjectOptions>
    <Version Value="12"/>
    <PathDelim Value="\"/>
    <General>
      <Flags>
        <MainUnitHasCreateFormStatements Value="False"/>
        <MainUnitHasTitleStatement Value="False"/>
        <MainUnitHasScaledStatement Value="False"/>
      </Flags>
      <SessionStorage Value="InProjectDir"/>
      <Title Value="simplecalc"/>
      <UseAppBundle Value="False"/>
      <ResourceType Value="res"/>
    </General>
    <LazDoc Paths="docs"/>
    <BuildModes>
      <Item Name="Default" Default="True"/>
    </BuildModes>
    <PublishOptions>
      <Version Value="2"/>
      <UseFileFilters Value="True"/>
    </PublishOptions>
    <RunParams>
      <FormatVersion Value="2"/>
    </RunParams>
    <Units>
      <Unit>
        <Filename Value="simplecalc.lpr"/>
        <IsPartOfProject Value="True"/>
      </Unit>
      <Unit>
        <Filename Value="tokenizer.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="Tokenizer"/>
      </Unit>
      <Unit>
        <Filename Value="tokenizertest.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="TokenizerTest"/>
      </Unit>
      <Unit>
        <Filename Value="units\shuntingyardparser.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="ShuntingYardParser"/>
      </Unit>
      <Unit>
        <Filename Value="units\testcase1.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="TestCase1"/>
      </Unit>
      <Unit>
        <Filename Value="mathoperations.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="MathOperations"/>
      </Unit>
      <Unit>
        <Filename Value="units\postfixcalculator.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="PostFixCalculator"/>
      </Unit>
    </Units>
  </ProjectOptions>
  <CompilerOptions>
    <Version Value="11"/>
    <PathDelim Value="\"/>
